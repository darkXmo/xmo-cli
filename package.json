{
  "name": "xmo-cli-mini",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "mitt": "^3.0.0",
    "pinia": "^2.0.0-rc.9",
