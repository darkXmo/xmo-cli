{
  "name": "xmo-cli-ts-mini",
  "version": "0.1.0",
  "license": "MIT",
  "description": "",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "commit": "git add . && cz",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^0.21.4",
    "pinia": "^2.0.0-rc.9",
    "sass": "^1.42.1",
    "vue": "^3.2.13",
    "vue-router": "4"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/node": "^16.10.1",
    "@vitejs/plugin-vue": "^1.9.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.0",
    "lint-staged": ">=10",
    "prettier": "2.4.1",
    "typescript": "^4.4.3",
    "vite": "^2.5.10",
    "vue-tsc": "^0.3.0"
  },
  "lint-staged": {
    "src/**/*.vue": [
      "prettier --write --ignore-unknown"
    ],
    "src/**/*.js": [
      "prettier --write --ignore-unknown"
    ],
    "src/**/*.ts": [
      "prettier --write --ignore-unknown"
    ],
    "*.{ts,js,css,md}": "prettier --write"
  }
}
